<!--pages/mine/mine.wxml-->
<view class="content">
  <view class="header">
    <view>
      <image src="images/banner.png" class="head"></image>
    </view>
    <view class="banner">
    <image class="via" src=""></image>
    <view class="message">
      <text>18372638734</text>
      <image></image>
    </view>
    <text class="name">一颗小葡萄</text>
  </view>
  </view>
  
  <!-- 余额 -->
  <view class="money">
    <view class="balance">
      <text class="difference">余额（元）</text>
      <text class="sum">18.00</text>
    </view>
    <view class="pay">
      <navigator class="up" hover-class="none">
        <text>充值</text>
      </navigator>
      <navigator class="to" hover-class="none">
        <text>提现</text>
      </navigator>
    </view>
  </view>
  <!-- 订单 -->
  <view class="bill">
    <navigator hover-class="none">
      <image src="images/ico.png"></image>
      <text>订单</text>
    </navigator>
    <navigator hover-class="none">
      <image src="images/ico.png"></image>
      <text>我的账单</text>
    </navigator>
    <navigator hover-class="none">
      <image src="images/ico.png"></image>
      <text>银行卡</text>
    </navigator>
    <navigator hover-class="none">
      <image src="images/ico.png"></image>
      <text>邀请好友</text>
    </navigator>
  </view>
  <!-- 功能 -->
  <view class="action">
    <navigator hover-class="none">
      <image src="images/icon.png"></image>
      <text>我的会员</text>
    </navigator>
    <navigator hover-class="none">
      <image src="images/icon.png"></image>
      <text>会员拼单</text>
    </navigator>
    <navigator hover-class="none">
      <image src="images/icon.png"></image>
      <text>企业商城</text>
    </navigator>
    <navigator hover-class="none">
      <image src="images/icon.png"></image>
      <text>交易记录</text>
    </navigator>
    <navigator hover-class="none">
      <image src="images/icon.png"></image>
      <text>意见反馈</text>
    </navigator>
    <navigator hover-class="none">
      <image src="images/icon.png"></image>
      <text>我的收货地址</text>
    </navigator>
  </view>
  <!-- 设置 -->
  <view class="set">
    <navigator hover-class="none">
      <image src="images/icon.png"></image>
      <text>设置</text>
    </navigator>
    <navigator hover-class="none">
      <image src="images/icon.png"></image>
      <text>关于富创</text>
    </navigator>
  </view>
  <view class="bottom">
    <navigator url="../index/index" hover-class="none">
      <image src="images/store.png"></image>
      <text>富创空间</text>
    </navigator>
    <navigator url="../store/store" hover-class="none">
      <image src="images/store.png"></image>
      <text>企业商城</text>
    </navigator>
    <navigator hover-class="none">
      <image src="images/store-active.png"></image>
      <text style="color:#E60012;">账户</text>
    </navigator>
  </view>
</view>
/**app.wxss**/

.container {
  height: 100%;
  /* display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between; */
  box-sizing: border-box;
  background: #f5f5f5;
}

.content {
  background: #f5f5f5;
}

.bottom {
  width: 100%;
  height: 100rpx;
  display: flex;
  background: #fff;
  border-top: 1px solid #ededed;
}

.bottom>navigator {
  width: 25%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.bottom>navigator>image {
  width: 56rpx;
  height: 56rpx;
}

.bottom>navigator>text {
  font-size: 20rpx;
}

.clearfloat {
  zoom: 1;
  overflow: hidden;
}